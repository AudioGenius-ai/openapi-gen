{
  "name": "openapi-gen",
  "version": "0.0.1",
  "description": "Custom OpenAPI Code Generator with Zod and React Query Hooks",
  "main": "dist/index.js",
  "bin": {
    "openapi-gen": "dist/cli.js"
  },
  "packageManager": "pnpm@10.11.0",
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "generate": "ts-node src/cli.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src",
    "prepublishOnly": "pnpm run build && pnpm run test:run",
    "example": "openapi-gen generate -i openapi.json -o generated",
    "example-dev": "openapi-gen generate -i openapi.json -o generated --base-url http://localhost:3000/api"
  },
  "keywords": [
    "openapi",
    "code-generation",
    "zod",
    "react-query",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.1",
    "@tanstack/react-query": "^5.79.0",
    "commander": "^14.0.0",
    "fs-extra": "^11.3.0",
    "openapi-types": "^12.1.3",
    "prettier": "^3.5.3",
    "zod": "^3.25.46"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.15.29",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "eslint": "^9.28.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  }
}
